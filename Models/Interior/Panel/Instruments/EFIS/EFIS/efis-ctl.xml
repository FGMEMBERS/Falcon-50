  <animation>
    <type>material</type>
    <object-name>texte</object-name>
    <object-name>RDR.wxr</object-name>
    <object-name>RDR.sta</object-name>
    <object-name>RDR.wpt</object-name>
    <object-name>RDR.arpt</object-name>
    <object-name>RDR.data</object-name>
    <object-name>RDR.pos</object-name>
    <object-name>RDR.terr</object-name>
    <object-name>DH.reset</object-name>
    <object-name>MINS.knob</object-name>
    <object-name>MINS.knob2</object-name>
    <object-name>inhg.reset</object-name>
    <object-name>BARO.knob</object-name>
    <object-name>BARO.knob2</object-name>
    <object-name>ND.center</object-name>
    <object-name>MODE.knob</object-name>
    <object-name>Range.knob</object-name>
    <object-name>TFC</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.20 </red>
      <green> 0.70 </green>
      <blue>  0.20 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>BARO.knob</object-name>
    <property>instrumentation/efis/inputs/kpa-mode</property>
    <factor> 80 </factor>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.041 </y-m>
      <z-m> 0.028 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Range.knob</object-name>
    <property>instrumentation/efis/inputs/range</property>
    <interpolation>
      <entry><ind>  10 </ind><dep>   0 </dep></entry>
      <entry><ind>  20 </ind><dep>  30 </dep></entry>
      <entry><ind>  40 </ind><dep>  60 </dep></entry>
      <entry><ind>  80 </ind><dep>  90 </dep></entry>
      <entry><ind> 160 </ind><dep> 120 </dep></entry>
      <entry><ind> 320 </ind><dep> 150 </dep></entry>
      <entry><ind> 640 </ind><dep> 180 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.022 </y-m>
      <z-m> -0.001 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>MINS.knob</object-name>
    <property>instrumentation/efis/inputs/minimums-mode</property>
    <factor> 80 </factor>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.041 </y-m>
      <z-m>  0.028 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>MODE.knob</object-name>
    <property>instrumentation/efis/mfd/mode-num</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 30 </dep></entry>
      <entry><ind> 2 </ind><dep> 60 </dep></entry>
      <entry><ind> 3 </ind><dep> 90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.020 </y-m>
      <z-m> -0.001 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LVOR.toggle</object-name>
    <property>instrumentation/efis/inputs/lh-vor-adf</property>
    <factor> 0.003 </factor>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.048 </y-m>
      <z-m> -0.000 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>RVOR.toggle</object-name>
    <property>instrumentation/efis/inputs/rh-vor-adf</property>
    <factor> 0.003 </factor>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.052 </y-m>
      <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

