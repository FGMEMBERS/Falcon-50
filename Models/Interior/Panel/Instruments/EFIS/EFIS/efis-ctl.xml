        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>instrumentation/inputs/setting-std</property>
            </binding>
			<binding>
				<condition>
					<property>instrumentation/inputs/setting-std</property>
				</condition>
                <command>property-assign</command>
                <property>instrumentation/efis/inhg-previos</property>
                <property>instrumentation/altimeter/setting-inhg</property>
            </binding>
			<binding>
				<condition>
					<property>instrumentation/inputs/setting-std</property>
				</condition>
                <command>property-assign</command>
				<property>instrumentation/altimeter/setting-inhg</property>
				<value>29.92</value>
            </binding>
			<binding>
