<?xml version="1.0" encoding="UTF-8"?>

<!-- Throttles -->

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    <object-name>manette1G</object-name>
    <object-name>manette2G</object-name>
    <object-name>manette3G</object-name>
    <object-name>manette1D</object-name>
    <object-name>manette2D</object-name>
    <object-name>manette3D</object-name>
    <!-- Objets transparents -->
  </animation>

 
  <!-- Throttle -->
  <animation>
    <type>translate</type>
    <object-name>manette1G</object-name>
    <object-name>manette1D</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> 0.10 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>manette2G</object-name>
    <object-name>manette2D</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> 0.10 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>manette3G</object-name>
    <object-name>manette3D</object-name>
    <property>controls/engines/engine[2]/throttle</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> 0.10 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

