  <model>
    <path>Aircraft/737-100/Models/light/flash.xml</path>
    <offsets>
      <x-m> -2.300 </x-m>
      <y-m>  0.664 </y-m>
      <z-m> -2.286 </z-m>
    </offsets>
  </model>

  <!-- Wing light cone right -->
  <model>
    <name>Right-Taxi-Light</name>
    <path>Aircraft/737-100/Models/light/light-cone.xml</path>
    <offsets>
      <x-m>          -2.300 </x-m>
      <y-m>           0.654 </y-m>
      <z-m>          -2.286 </z-m>
      <heading-deg>  -10 </heading-deg>
      <pitch-deg>    -10 </pitch-deg>
    </offsets>
  </model>

  <animation>
    <name>All-Taxi-Light</name>
    <object-name>Left-Taxi-Light</object-name>
    <object-name>Right-Taxi-Light</object-name>
  </animation>

  <animation>
    <type>select</type>
    <object-name>All-Taxi-Light</object-name>
    <condition>
      <equals>
        <property>controls/switches/landing-light</property>
        <value> 1 </value>
      </equals>
    </condition>
  </animation>

</PropertyList>

